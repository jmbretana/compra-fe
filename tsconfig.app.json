{
  "compilerOptions": {
    "paths": {
      "@actions": ["src/middleware/actions"],
      "@actions/*": ["src/middleware/actions/*"],
      "@auth": ["src/auth"],
      "@auth/*": ["src/auth/*"],
      "@common": ["src/components/common"],
      "@common/*": ["src/components/common/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@enums": ["src/interfaces/enum"],
      "@enums/*": ["src/interfaces/enum/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@interfaces": ["src/interfaces"],
      "@interfaces/*": ["src/interfaces/*"],
      "@layout": ["src/layout"],
      "@layout/*": ["src/layout/*"],
      "@middleware": ["src/middleware"],
      "@middleware/*": ["src/middleware/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@public": ["public"],
      "@public/*": ["public/*"],
      "@redux": ["src/middleware/redux"],
      "@redux/*": ["src/middleware/redux/*"],
      "@router": ["src/router"],
      "@router/*": ["src/router/*"],
      "@store": ["src/middleware/store"],
      "@store/*": ["src/middleware/store/*"],
      "@theme": ["src/components/common/theme"],
      "@theme/*": ["src/components/common/theme/*"],
      "@types": ["src/middleware/types"],
      "@types/*": ["src/middleware/types/*"],

      "@AccountActionTypes": ["src/middleware/types/AccountActionTypes"],
      "@AmountActionTypes": ["src/middleware/types/AmountActionTypes"],
      "@BrandActionTypes": ["src/middleware/types/BrandActionTypes"],
      "@BookmarkActionTypes": ["src/middleware/types/BookmarkActionTypes"],
      "@CartActionTypes": ["src/middleware/types/CartActionTypes"],
      "@GeoActionTypes": ["src/middleware/types/GeoActionTypes"],
      "@ListActionTypes": ["src/middleware/types/ListActionTypes"],
      "@OrderActionTypes": ["src/middleware/types/OrderActionTypes"],
      "@ProductActionTypes": ["src/middleware/types/ProductActionTypes"],
      "@ProviderActionTypes": ["src/middleware/types/ProviderActionTypes"],
      "@SnackActionTypes": ["src/middleware/types/SnackActionTypes"],
      "@UserActionTypes": ["src/middleware/types/UserActionTypes"],

      "@config": ["src/config"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@values": ["src/values"],
      "@values/*": ["src/values/*"]
    }
  },
  "include": ["src"]
}
